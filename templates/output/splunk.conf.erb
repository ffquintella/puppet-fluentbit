# Managed by puppet
[OUTPUT]
    Name                   splunk
<% if @match -%>
    Match                  <%= @match %>
<% end -%>
<% if @host -%>
    Host                   <%= @host %>
<% end -%>
<% if @http_user -%>
    HTTP_User              <%= @http_user %>
<% end -%>
<% if @http_passwd -%>
    HTTP_Passwd            <%= @http_passwd %>
<% end -%>
<% if @port -%>
    Port                   <%= @port %>
<% end -%>
<% if @tls_enabled -%>
    tls                    <%= @tls_enabled %>
<% end -%>
<% if @tls_verify -%>
    tls.verify             <%= @tls_verify %>
<% end -%>
<% if @tls_debug -%>
    tls.debug              <%= @tls_debug %>
<% end -%>
<% if @tls_ca_file -%>
    tls.ca_file            <%= @tls_ca_file %>
<% end -%>
<% if @tls_ca_path -%>
    tls.ca_path            <%= @tls_ca_path %>
<% end -%>
<% if @tls_crt_file -%>
    tls.crt_file           <%= @tls_crt_file %>
<% end -%>
<% if @tls_key_file -%>
    tls.key_file           <%= @tls_key_file %>
<% end -%>
<% if @tls_key_passwd -%>
    tls.key_passwd         <%= @tls_key_passwd %>
<% end -%>
<% if @splunk_token -%>
    Splunk_Token           <%= @splunk_token %>
<% end -%>
